{
  "addons": [
    "mongolab"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ],
  "env": {
    "MONGO_DB": "sosad_staging_db",
    "MONGO_PASS": {
      "required": true
    },
    "MONGO_PORT": {
      "required": true
    },
    "MONGO_USER": {
      "required": true
    },
    "MONGODB_URI": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "site",
  "scripts": {
  },
  "stack": "heroku-18"
}
